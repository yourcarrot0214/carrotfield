hangeProfileImage\n    let profileImageURL = \"\";\n\n    if (PhotoURL !== UserObject.photoURL) {\n      // attachment image가 있으면 사진 경로를 지정\n      const attachmentRef = firebaseStorage\n        .ref()\n        .child(`${UserObject.uid}/${uuidv4()}`);\n      const response = await attachmentRef.putString(PhotoURL, \"data_url\");\n      profileImageURL = await response.ref.getDownloadURL();\n      // update\n      await UserObject.updateProfile({\n        photoURL: profileImageURL,\n      });\n      refreshUser();\n      setPhotoURL(profileImageURL);\n    }\n  };\n\n  const onChange = (event) => {\n    const { value } = event.target;\n    setNewDisplayName(value);\n  };\n\n  const onFileChange = (event) => {\n    const { files } = event.target;\n    const theFile = files[0];\n    const reader = new FileReader();\n    reader.onloadend = (finishedEvent) => {\n      const { result } = finishedEvent.currentTarget;\n      setPhotoURL(result);\n    };\n    if (theFile) reader.readAsDataURL(theFile);\n  };\n\n  return (\n    <div className=\"container\">\n      {PhotoURL ? (\n        <img className=\"profile__img\" alt=\"프로필 이미지\" src={PhotoURL} />\n      ) : (\n        <div className=\"profile__img__container\">\n          <FontAwesomeIcon icon={faUserCircle} size=\"6x\" />\n        </div>\n      )}\n      <form onSubmit={onUpdateProfile} className=\"profileForm\">\n        <label htmlFor=\"image-file\" className=\"profile__label\">\n          <span>Change Profile Image</span>\n          <FontAwesomeIcon icon={faPlus} />\n        </label>\n        <input\n          id=\"image-file\"\n          type=\"file\"\n          accept=\"image/*\"\n          onChange={onFileChange}\n          style={{ opacity: 0 }}\n        />\n        <input\n          type=\"text\"\n          placeholder=\"Display Name\"\n          value={NewDisplayName}\n          onChange={onChange}\n          className=\"formInput\"\n          maxLength={15}\n        />\n        <input\n          type=\"submit\"\n          value=\"Update Profile\"\n          className=\"formBtn\"\n          style={{ marginTop: 10 }}\n        />\n      </form>\n      <span className=\"formBtn cancelBtn logOut\" onClick={onLogOut}>\n        Log Out\n      </span>\n    </div>\n  );\n};\n\nexport default Profile;\n\n/*\n  onSubmit function > update profile으로 통합\n  displayName 변경시 updateDisplayName()\n  photoURL 변경시 updatePhotoURL()\n*/\n",["76","77"],"/Users/carrot/Desktop/react-clone-twitter/src/index.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/Router.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/routes/TweetPage.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/TweetForm.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/AuthForm.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/SocialSignInForm.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/TweetOptions.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/Tweet.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/TweetEditor.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/routes/Loader.js",["78"],"/Users/carrot/Desktop/react-clone-twitter/src/redux/reduxStore.js",["79"],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":2,"column":41,"nodeType":"86","messageId":"87","endLine":2,"endColumn":54},{"ruleId":"80","replacedBy":"88"},{"ruleId":"82","replacedBy":"89"},{"ruleId":"84","severity":1,"message":"90","line":3,"column":8,"nodeType":"86","messageId":"87","endLine":3,"endColumn":15},{"ruleId":"84","severity":1,"message":"91","line":1,"column":10,"nodeType":"86","messageId":"87","endLine":1,"endColumn":21},"no-native-reassign",["92"],"no-negated-in-lhs",["93"],"no-unused-vars","'firebaseStore' is defined but never used.","Identifier","unusedVar",["92"],["93"],"'loading' is defined but never used.","'createStore' is defined but never used.","no-global-assign",