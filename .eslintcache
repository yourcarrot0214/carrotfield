eReader();\n    reader.onloadend = (finishedEvent) => {\n      const { result } = finishedEvent.currentTarget;\n      setPhotoURL(result);\n    };\n    if (theFile) reader.readAsDataURL(theFile);\n  };\n\n  return (\n    <div className=\"container\">\n      {PhotoURL ? (\n        <img className=\"profile__img\" alt=\"프로필 이미지\" src={PhotoURL} />\n      ) : (\n        <div className=\"profile__img__container\">\n          <FontAwesomeIcon icon={faUserCircle} size=\"6x\" />\n        </div>\n      )}\n      <form onSubmit={onUpdateProfile} className=\"profileForm\">\n        <label htmlFor=\"image-file\" className=\"profile__label\">\n          <span>Change Profile Image</span>\n          <FontAwesomeIcon icon={faPlus} />\n        </label>\n        <input\n          id=\"image-file\"\n          type=\"file\"\n          accept=\"image/*\"\n          onChange={onFileChange}\n          style={{ opacity: 0 }}\n        />\n        <input\n          type=\"text\"\n          placeholder=\"Display Name\"\n          value={NewDisplayName}\n          onChange={onChange}\n          className=\"formInput\"\n          maxLength={15}\n        />\n        <input\n          type=\"submit\"\n          value=\"Update Profile\"\n          className=\"formBtn\"\n          style={{ marginTop: 10 }}\n        />\n      </form>\n      <span className=\"formBtn cancelBtn logOut\" onClick={onLogOut}>\n        Log Out\n      </span>\n    </div>\n  );\n};\n\nexport default Profile;\n\n/*\n  onSubmit function > update profile으로 통합\n  displayName 변경시 updateDisplayName()\n  photoURL 변경시 updatePhotoURL()\n*/\n",["100","101"],"/Users/carrot/Desktop/react-clone-twitter/src/index.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/Router.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/routes/TweetPage.js",["102"],"/Users/carrot/Desktop/react-clone-twitter/src/components/TweetForm.js",[],"/Users/carrot/Desktop/react-clone-twitter/src/components/AuthForm.js",[],"/Users/carrot/Desktop/rea